datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId 
  email String @unique
  password String
  name String
  todos Todo[]
}